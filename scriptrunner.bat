@echo off
title Oculus Toolkit

echo Command Prompt Menu
echo ===================
echo.
echo 1) Normal mode
echo 2) Oculus Quest 1
echo 3) Oculus Quest 2
echo 4) Android screen sharing
echo 5) Full VR view (both eyes)
echo 6) Install APK
echo 7) Uninstall APK
echo 8) Take a screenshot
echo 9) Restart device
echo 10) Exit
echo ===================
echo.

setlocal enabledelayedexpansion

:menu
set "option="
set /p "option=Enter the number of your option (1-10): "

if "%option%"=="1" (
    echo Running Normal mode command
    scrcpy
) else if "%option%"=="2" (
    echo Running Oculus Quest 1 command
    scrcpy --crop 1280:720:1500:350
) else if "%option%"=="3" (
    echo Running Oculus Quest 2 command
    scrcpy --crop 1600:900:2017:510
) else if "%option%"=="4" (
    echo Running Android screen sharing command
    scrcpy
) else if "%option%"=="5" (
    echo Running Full VR view command
    scrcpy
) else if "%option%"=="6" (
    call :installAPK
) else if "%option%"=="7" (
    call :uninstallAPK
) else if "%option%"=="8" (
    call :takeScreenshot
) else if "%option%"=="9" (
    call :restartDevice
) else if "%option%"=="10" (
    echo Exiting...
    exit /b
) else (
    echo Invalid option
    goto menu
)

endlocal
exit /b

:installAPK
echo.
echo Installing APK using ADB

REM Same installation code as before

goto menu

:uninstallAPK
echo.
echo Uninstalling APK using ADB

set "adbPath="
set "packageName="

echo Locating adb...
for /f "delims=" %%a in ('where adb') do (
    set "adbPath=%%a"
    echo adb found: !adbPath!
    goto :next
)

:next
if not defined adbPath (
    echo adb not found. Make sure adb is installed and added to the system's PATH environment variable.
    echo.
    pause
    goto menu
)

echo.
echo Fetching installed packages...
%adbPath% shell pm list packages -3 > "%TEMP%\packages.txt"

echo.
echo Select the package number to uninstall:
echo.

setlocal enabledelayedexpansion
set /a "index=0"

for /f "usebackq delims=" %%a in ("%TEMP%\packages.txt") do (
    set /a "index+=1"
    set "package=%%a"
    echo !index!) !package:~8!
)

endlocal

set /p "selection=Enter the number of the package to uninstall: "

setlocal enabledelayedexpansion
set "targetPackage="
for /f "usebackq tokens=1,* delims=:" %%a in ("%TEMP%\packages.txt") do (
    set /a "index+=1"
    if "!index!"=="%selection%" (
        set "targetPackage=%%b"
    )
)
endlocal

if not defined targetPackage (
    echo Invalid package selection.
) else (
    echo Uninstalling package: !targetPackage!
    %adbPath% uninstall !targetPackage!
)

del "%TEMP%\packages.txt"

goto menu

:takeScreenshot
echo.
echo Taking a screenshot using ADB

set "adbPath="
set "screenshotPath="

echo Locating adb...
for /f "delims=" %%a in ('where adb') do (
    set "adbPath=%%a"
    echo adb found: !adbPath!
    goto :next
)

:next
if not defined adbPath (
    echo adb not found. Make sure adb is installed and added to the system's PATH environment variable.
    echo.
    pause
    goto menu
)

echo.
echo Browse and select the location to save the screenshot:
echo.

set "powershellCmd=[System.Reflection.Assembly]::LoadWithPartialName('System.Windows.Forms') | Out-Null; $fileDialog = New-Object System.Windows.Forms.SaveFileDialog; $fileDialog.Filter = 'PNG files (*.png)|*.png'; $fileDialog.ShowDialog() | Out-Null; $fileDialog.FileName"
for /f "delims=" %%a in ('powershell -Command "%powershellCmd%"') do (
    set "screenshotPath=%%a"
)

if not defined screenshotPath (
    echo Screenshot selection canceled.
) else (
    echo Taking a screenshot...
    %adbPath% shell screencap -p /sdcard/screenshot.png
    %adbPath% pull /sdcard/screenshot.png "%screenshotPath%"
    echo Screenshot saved: %screenshotPath%
)

goto menu

:restartDevice
echo.
echo Restarting the connected device using ADB

set "adbPath="

echo Locating adb...
for /f "delims=" %%a in ('where adb') do (
    set "adbPath=%%a"
    echo adb found: !adbPath!
    goto :next
)

:next
if not defined adbPath (
    echo adb not found. Make sure adb is installed and added to the system's PATH environment variable.
    echo.
    pause
    goto menu
)

echo Restarting the device...
%adbPath% reboot

goto menu
